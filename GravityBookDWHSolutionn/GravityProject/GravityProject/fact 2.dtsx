                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH OrderAggregates AS (
  SELECT 
    ol.order_id,
    MIN(ol.book_id) AS book_id,
    CAST(SUM(ol.price) AS DECIMAL(10,2)) AS books_cost,
    COUNT(ol.book_id) AS book_count
  FROM order_line ol
  GROUP BY ol.order_id
),
OrderInfo AS (
  SELECT
    co.order_id,
    co.customer_id,
    sm.method_id,
    CAST(MAX(sm.cost) AS DECIMAL(10,2)) AS shipping_cost,
    MAX(co.order_date) AS order_date
  FROM cust_order co
  INNER JOIN shipping_method sm ON co.shipping_method_id = sm.method_id
  GROUP BY co.order_id, co.customer_id, sm.method_id
),
OrderStatus AS (
  SELECT order_id, MIN(status_id) AS status_id
  FROM order_history
  GROUP BY order_id
)

SELECT 
  a.order_id,
  i.customer_id,
  a.book_id,
  s.status_id,
  i.method_id,
  i.order_date,
  a.books_cost,
  i.shipping_cost,
 CAST(a.books_cost + i.shipping_cost AS DECIMAL(10,2)) AS total_cost
,

  a.book_count
FROM OrderAggregates a
INNER JOIN OrderInfo i ON a.order_id = i.order_id
INNER JOIN OrderStatus s ON a.order_id = s.order_id;</property>
